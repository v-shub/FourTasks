<Window x:Class="Ex4.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ex4"
        mc:Ignorable="d"
        Title="Window2" Height="450" Width="800">
    <Grid>
        <Button x:Name="ToMainButton" Content="Назад" HorizontalAlignment="Left" Height="28" Margin="24,378,0,0" VerticalAlignment="Top" Width="93" Click="Button_Click"/>
        <TextBlock HorizontalAlignment="Center" Height="39" Margin="0,91,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"><Run Language="ru-ru" Text="Вывести квадраты всех чисел от 10 до 20?"/></TextBlock>
        <Button x:Name="CalcButton" Content="Да" HorizontalAlignment="Center" Height="27" Margin="0,135,0,0" VerticalAlignment="Top" Width="44" Click="Button_Click_1"/>
        <Label x:Name="ResultLabel" Content="Здесь будет результат" HorizontalAlignment="Center" Height="33" VerticalAlignment="Center" Width="274"/>

    </Grid>
</Window>
